'From Squeak5.3 of 3 March 2020 [latest update: #19431] on 14 April 2023 at 11:10:50 pm'!TestCase subclass: #PolynomTest	instanceVariableNames: 'p1 p2 p3 p4 m1 m2 m3 m4'	classVariableNames: ''	poolDictionaries: ''	category: 'OOP1'!!PolynomTest methodsFor: 'testing' stamp: 'a 4/14/2023 23:10'!test |temp_dict|	p1 := Polynom new.	p2 := Polynom new.	p3 := Polynom new.	p4 := Polynom new.	m1 := Monom new.	m2 := Monom new.	m1 coef:3.	m1 exp:3.		p1 addMonom: m1.	self assert: ((p1 eval:3)=81).	m1:=m1 derivative.	self assert: ((p1 eval:3)=81).	p1 addMonom: m1.	self assert: ((p1 eval:3)=162).	m2 coef:1.	m2 exp:1.			p1 multiplyByMonom:m2.	self assert: ((p1 eval:2)=120).	m2 coef:2.	m2 exp:2.		self assert: ((p1 eval:2)=120).	temp_dict := Dictionary new.	temp_dict add: 3->9; add: 4->3.	self assert: ((p1 asDictionary) = temp_dict).	p3 := p1 derivative.	self assert: ((p3 eval:1)=39).	p4 := (p3 add:p1).	self assert: ((p3 eval:1)=39).	self assert: ((p4 eval:1)=51).	p3 := p3 derivative.	p1 := p1 derivative.	self assert: ((p4 eval:2)=324).	temp_dict := Dictionary new.	temp_dict add: 2->27; add: 3->12.	self assert: ((p1 asDictionary) = temp_dict).	temp_dict := Dictionary new.	temp_dict add: 1->54; add: 2->36.	"Transcript show: (p3 asDictionary); cr."	self assert: ((p3 asDictionary) = temp_dict).			temp_dict := Dictionary new.	temp_dict add: 2->27; add: 3->21; add: 4->3.	self assert: ((p4 asDictionary) = temp_dict).			! !